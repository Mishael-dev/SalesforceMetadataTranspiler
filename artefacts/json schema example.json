    {
      "label": "Asset",
      "pluralLabel": "Assets",
      "description": "Represents a company asset used for tracking inventory and lifecycle status.",
      "deploymentStatus": "Deployed",
      "allowInChatterGroups": true,
      "nameField": {
        "label": "Asset Name",
