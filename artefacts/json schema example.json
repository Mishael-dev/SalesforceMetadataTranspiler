      "visibility": "Public",
      "fields": [
        {
          "type": "AutoNumber",
          "label": "Asset ID",
          "fullName": "Asset_ID__c",
          "displayFormat": "ASSET-{YYYY}-{0000}",
          "description": "Automatically generated identifier for each asset record, including year of creation.",
          "helpText": "This field is auto-generated by Salesforce. Example: ASSET-2026-0001",
          "startingNumber": 1
        },
        {
          "type": "Formula",
          "label": "Asset Status Label",
          "fullName": "Asset_Status_Label__c",
          "returnType": "Text",
          "formula": "IF(NOT(ISBLANK(Asset_ID__c)), \"ACTIVE: \" & Name, \"INACTIVE\")",
          "blankOption": "BlankAsZero",
          "description": "Displays a human-readable status based on whether the asset has been assigned an ID.",
          "helpText": "Shows ACTIVE or INACTIVE followed by the record name.",
          "externalId": false,
          "required": false,
          "unique": false,
          "trackHistory": false
        }
      ]
